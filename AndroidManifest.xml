<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.fu.xml.xread"
    android:versionCode="1"
    android:versionName="1.0">
    
    <uses-permission android:name="android.permission.INTERNET" /> 

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" >
        
        <activity
            android:name="de.fu.xml.xread.activities.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.WebActivity"></activity>
        <activity android:name=".activities.HistoryActivity"></activity>
        <activity android:name=".activities.GeoActivity"></activity>
        <activity android:name=".activities.DBPediaActivity"></activity>
        <activity android:name=".activities.StackoverflowActivity"></activity>
        <activity android:name=".activities.TwitterActivity"></activity>
        <activity android:name=".activities.SparqlActivity"></activity>
         
    </application>

</manifest>
